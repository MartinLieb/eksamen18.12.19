extends layout

block content
    h1= "Resurser til HjemlÃ¸se"
    div.container
        table.table.table-striped
            thead
                th= "Id"
                th= "By"
                th= "Kategori Navn"
                th= "Beskrivelse"
                th= "Adresse"
                th= "Telefon"
                th= "Aabningstider"
                th= "Webside"
