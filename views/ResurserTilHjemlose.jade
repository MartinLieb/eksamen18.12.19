                th= "Updated At"
            tbody
                each resurser in resursers
                    tr
                        td= resurser.id
                        td= resurser.by
                        td= resurser.kategori_navn
                        td= resurser.beskrivelse
                        td= resurser.adresse
                        td= resurser.telefon
                        td= resurser.aabningstider
                        td= resurser.webside
                        td= resurser.createdAt
                        td= resurser.updatedAt